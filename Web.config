<configuration>
	<system.webServer>
		<staticContent>
			<mimeMap fileExtension=".webp" mimeType="image/webp" />
		</staticContent>
	</system.webServer>

	<connectionStrings>
		<add name="travelzCnn" connectionString="Data Source=CODEROM; Initial Catalog=HotelsToStay;Integrated Security=True;" providerName="System.Data.SqlClient" />
	</connectionStrings>

	<system.web>
		<compilation debug="true" targetFramework="4.5"/>
		<pages validateRequest="false" />
		<httpRuntime requestValidationMode="2.0" />
	</system.web>

</configuration>